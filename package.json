{
  "name": "lsk-build",
  "version": "0.10.4",
  "description": "Lego Starter Kit builder & webpack configurator",
  "main": "index.js",
  "scripts": {
    "release": "npm run babel-build && npm publish dist",
    "babel-build": "rm -rf dist && mkdir dist && cp {package.json,yarn.lock,README.md} dist/ && node_modules/.bin/babel src --out-dir dist --source-maps inline --copy-files",
    "dev": "DEBUG=1 npm run babel-build -- --watch"
  },
